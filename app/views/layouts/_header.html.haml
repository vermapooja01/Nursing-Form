%nav.navbar.navbar-default
  .container
    / Brand and toggle get grouped for better mobile display
    .navbar-header.col-md-3
      %button.navbar-toggle.collapsed{"aria-expanded" => "false", "data-target" => "#miner-chart-nav", "data-toggle" => "collapse", :type => "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a.navbar-brand.col-md-12{:href => "#{root_url}"}
        = image_tag('minerChart_logo.svg')

    / Collect the nav links, forms, and other content for toggling
    #miner-chart-nav.navbar-collapse
      %ul.nav.navbar-nav
        %li
          = link_to 'Home', root_url
        - if !logged_in?
          %li
            = link_to 'Log In', signin_path
        - else
          %li
            = link_to 'Log Out', sign_out_path





