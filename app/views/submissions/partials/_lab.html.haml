  = f.input :coagulation_therapy, :collection => [['yes', 'Yes'],['no', 'No']], :label_method => :last, :value_method => :first, :as => :radio_buttons, :item_wrapper_class => 'inline', :checked => @submission.coagulation_therapy, :class => "form-control", :label => "Is the Patient currently on Coagulation Therapy"
