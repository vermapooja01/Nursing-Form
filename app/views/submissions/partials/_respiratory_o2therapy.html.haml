  = f.input :location_where, :class => "form-control"

.col-md-12
  %h1 Suction
  = f.input :trach_type, :class => "form-control"
  = f.input :suction_type, :collection => [['air leak', 'Air Leak'], ['sub q emphysema', 'Sub Q Emphysema']], :label_method => :last, :value_method => :first, :as => :radio_buttons, :item_wrapper_class => 'inline', :checked => true, :class => "form-control"
  = f.input :emphysema_where, :class => "form-control"

.col-md-12
  = f.input :chest_tube_comments, :class => "form-control"








